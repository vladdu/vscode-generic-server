{
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "args": ["run"],
  "showOutput": "always",
  "tasks": [
    {
      "taskName": "build",
      "isBuildCommand": true,
      "problemMatcher": "$tsc-watch"
    },
    {
      "taskName": "clean"
    },
    {
      "taskName": "lint"
    },
    {
      "taskName": "coverage"
    },
    {
      "taskName": "test",
      "isTestCommand": true
    }
  ]
}
